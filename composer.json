{
    "name": "peterhebert/wp-composer-sync",
    "description": "A WP-CLI command to scan an existing WordPress site and sync active plugins, themes, and WP core as dependencies into composer.json.",
    "type": "wp-cli-package",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Peter Hebert",
            "email": "peter@peterhebert.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "PeterHebert\\WPComposerSync\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PeterHebert\\WPComposerSync\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.4",
        "wp-cli/wp-cli": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "brain/monkey": "^2.6",
        "mikey179/vfsstream": "^1.6",
        "mockery/mockery": "^1.5"
    },
    "extra": {
        "wp-cli": {
            "main": "package.php"
        }
    }
}